{
	"name": "vismark",
	"version": "2.0.0",
	"description": "Svelte component library including Mdsvex preprocessor to support creating complex webpages focusing on data visualisation.",
	"homepage": "https://github.com/lea-8/Vismark",
	"bugs": {
		"url": "https://github.com/lea-8/Vismark/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lea-8/Vismark"
	},
	"author": "Lea A",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"format": "prettier --write ./src",
		"format-check": "prettier --check ./src",
		"lint-auto": "eslint ./src --format json --output-file testing/reports/eslint-report.json && prettier --check .",
		"lint": "eslint ./src --format json --output-file testing/reports/eslint-report.json",
		"lint-test": "eslint ./README.md",
		"jsdoc": "./node_modules/jsdoc/jsdoc.js src/lib/*"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./config": "./dist/preprocessorConfigHelper.js",
		"./AreaChart": {
			"types": "./dist/components/charts/AreaChart.svelte.d.ts",
			"svelte": "./dist/components/charts/AreaChart.svelte"
		},
		"./BarChart": {
			"types": "./dist/components/charts/BarChart.svelte.d.ts",
			"svelte": "./dist/components/charts/BarChart.svelte"
		},
		"./LineChart": {
			"types": "./dist/components/charts/LineChart.svelte.d.ts",
			"svelte": "./dist/components/charts/LineChart.svelte"
		},
		"./Histogram": {
			"types": "./dist/components/charts/Histogram.svelte.d.ts",
			"svelte": "./dist/components/charts/Histogram.svelte"
		},
		"./ScatterPlot": {
			"types": "./dist/components/charts/ScatterPlot.svelte.d.ts",
			"svelte": "./dist/components/charts/ScatterPlot.svelte"
		},
		"./BoxPlot": {
			"types": "./dist/components/charts/BoxPlot.svelte.d.ts",
			"svelte": "./dist/components/charts/BoxPlot.svelte"
		},
		"./Header": {
			"types": "./dist/components/pageElements/Header.svelte.d.ts",
			"svelte": "./dist/components/pageElements/Header.svelte"
		},
		"./Footer": {
			"types": "./dist/components/pageElements/Footer.svelte.d.ts",
			"svelte": "./dist/components/pageElements/Footer.svelte"
		},
		"./Grid": {
			"types": "./dist/components/pageElements/Grid.svelte.d.ts",
			"svelte": "./dist/components/pageElements/Grid.svelte"
		},
		"./Cell": {
			"types": "./dist/components/pageElements/Cell.svelte.d.ts",
			"svelte": "./dist/components/pageElements/Cell.svelte"
		},
		"./Widget": {
			"types": "./dist/components/pageElements/Widget.svelte.d.ts",
			"svelte": "./dist/components/pageElements/Widget.svelte"
		},
		"./Image": {
			"types": "./dist/components/pageElements/Image.svelte.d.ts",
			"svelte": "./dist/components/pageElements/Image.svelte"
		},
		"./Dropdown": {
			"types": "./dist/components/text/Dropdown.svelte.d.ts",
			"svelte": "./dist/components/text/Dropdown.svelte"
		}
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@eslint/json": "^0.13.1",
		"@eslint/markdown": "^7.1.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/package": "^2.4.1",
		"@sveltejs/vite-plugin-svelte": "^2.5.2",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-formatter-json": "^8.40.0",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"jsdoc": "^4.0.4",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"typescript": "^5.3.2",
		"vite": "^4.0.0"
	},
	"keywords": [
		"svelte",
		"observable plot",
		"markdown"
	],
	"dependencies": {
		"@observablehq/plot": "^0.6.17",
		"@sveltejs/kit": "^1.30.4",
		"d3": "^7.9.0",
		"mdsvex": "^0.12.6",
		"rehype-katex": "^5.0.0",
		"remark-footnotes": "^2.0.0",
		"remark-math": "^3.0.0",
		"svelte": "^4.2.7",
		"unist-util-visit": "^2.0.3"
	}
}
